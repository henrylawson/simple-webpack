{
  "name": "libraye-bookeye",
  "version": "0.0.0",
  "description": "Libraye Bookeye",
  "scripts": {
    "clean": "node_modules/.bin/rimraf public/main.*.*",
    "build": "node_modules/.bin/webpack --config ./client/conf/webpack.config.js",
    "build:watch": "npm run build -- --watch",
    "test": "cross-env NODE_ENV=test mocha ./client/spec/**/*.spec.js --recursive --compilers js:babel-register --require ./client/spec/setup.js",
    "test:watch": "npm test -- --watch",
    "lint": "node_modules/.bin/esw ./client/conf/webpack.config.js ./client/lib",
    "lint:watch": "npm run lint -- --watch"
  },
  "homepage": "http://redux.js.org",
  "dependencies": {
    "babel-polyfill": "^6.7.4",
    "classnames": "^2.2.5",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.4.0",
    "redux-form": "^5.0.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.1",
    "eslint-plugin-react": "^5.2.2",
    "eslint-watch": "^2.1.11",
    "expect": "^1.16.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "jsdom": "^8.4.0",
    "mocha": "^2.4.5",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.10.0",
    "react-addons-test-utils": "^15.0.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1"
  }
}
